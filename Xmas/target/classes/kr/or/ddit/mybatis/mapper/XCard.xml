<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.member.dao.XmasCardDAO">

	<select id="selectOneXCard" resultType="XmasCardVO" parameterType="int">
		SELECT
			XC_NO
			, XC_SENDER
			, XC_RECEIVER
			, XC_TITLE
			, XC_CONTENT
			, XC_DATE
		FROM
			TB_XMASCARD
			    		
	</select>
	
	
</mapper>

			System.out.println(simpleVO.getAddress());
		}
		
		SimpleVO simpleVO2 = new SimpleVO();
		simpleVO2.setName("홍길동3");
		simpleVO2.setAddress("대전3");
		
		System.out.println(simpleVO2.getName());
		System.out.println(simpleVO2.getAddress());
		System.out.println("contains : " + simpleVOList.contains(simpleVO2));
	
